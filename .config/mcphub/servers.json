{
  "nativeMCPServers": [
    
  ],
  "mcpServers": {
    "git": {
      "disabled_tools": [
        
      ],
      "disabled": false,
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    },
    "github": {
      "disabled": false,
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "cmd:op read 'op://personal/Codecompanion Github MCP/credential' --no-newline"
      },
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ]
    },
    "context7": {
      "custom_instructions": {
        "disabled": false
      },
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    }
  }
}